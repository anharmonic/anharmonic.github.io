header li {
	list-style-type: none;
  width:132px;
  height:15px;
	margin-bottom: 12px;
	line-height: 1em;
	padding: 6px 6px 6px 7px;

	background: #6f02ec;
	background: -moz-linear-gradient(top, #ab1752 0%, #6f02ec 100%);
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f8f8f8), color-stop(100%,#dddddd));
  background: -webkit-linear-gradient(top, #ab1752 0%,#6f02ec 100%);
  background: -o-linear-gradient(top, #ab1752 0%,#6f02ec 100%);
  background: -ms-linear-gradient(top, #ab1752 0%,#6f02ec 100%);
  background: linear-gradient(to top, #ab1752 0%,#6f02ec 100%);

	border-radius:4px;
  border:1px solid #8f0d99;

	-webkit-box-shadow: inset 0px 1px 1px 0 #8f0d99;
	box-shadow: inset 0px 1px 1px 0 #8f0d99;

}
header li:hover {
	background: #4c01a7;
	background: -moz-linear-gradient(top, #871241 0%, #4c01a7 100%);
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f8f8f8), color-stop(100%,#dddddd));
  background: -webkit-linear-gradient(top, #871241 0%,#4c01a7 100%);
  background: -o-linear-gradient(top, #871241 0%,#4c01a7 100%);
  background: -ms-linear-gradient(top, #871241 0%,#4c01a7 100%);
  background: linear-gradient(to top, #871241 0%,#4c01a7 100%);
}


h1{
  color: #ac1847;
}

h2{
  color: #a5137b;
}

h3{
  color: #dc6900;
}

sup{
vertical-align: 15%;
    font-size: 60%;
}
