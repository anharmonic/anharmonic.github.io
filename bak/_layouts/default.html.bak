<!doctype html>
<html lang="{{ site.lang | default: "en-US" }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

{% seo %}
    <link rel="stylesheet" href="{{ '/assets/css/style.css?v=' | append: site.github.build_revision | relative_url }}">
    <script src="{{ '/assets/js/scale.fix.js' | relative_url }}"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="wrapper">
      <header>
        <h1 class="header">{{ site.title | default: site.github.repository_name }}</h1>
        <p class="header">{{ site.description | default: site.github.project_tagline }}</p>

        <ul>
          <li><a class="buttons github" href="https://github.com/anharmonic/d3q">View On GitHub</a></li>
     <li class="navlink"><a class="buttons navlink" href="https://anharmonic.github.io/">Home&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</a></li>
     <li class="navlink"><a class="buttons navlink" href="https://anharmonic.github.io/started">Getting started</a></li>
     <li class="navlink"><a class="buttons navlink" href="https://anharmonic.github.io/thermal2">Thermal2&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</a></li>
     <li class="navlink"><a class="buttons navlink" href="https://anharmonic.github.io/d3q">D3Q&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</a></li>
        </ul>
        <!--{% include nav.html %}-->
      </header>

      <section>
        <!--<h1 class="htitle">{{ site.title | default: site.github.repository_name }}</h1>
        <h1 class="hsubtitle">{{ site.subtitle | default: site.github.repository_name }}</h1>-->
        {{ content }}
      </section>

      <footer>
        <p class="header" style="font-size:0.8em;"><small>This project is maintained by <a href="https://github.com/paulatz">Lorenzo&nbsp;Paulatto</a></small></p>
        <!--<p><small>Hosted on <a href="https://pages.github.com">GitHub Pages</a> using the Dinky theme</small></p>-->
      </footer>
    </div>
  </body>
</html>
